{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "exclude": ["**/node_modules/**"],
  "rules": {
    "one-variable-per-declaration": false,
    "semicolon": false,
    "quotemark": "double",
    "variable-name": ["allow-pascal-case"],
    "indent": false,
    "import-name": false
  }
}
