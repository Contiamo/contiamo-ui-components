{
  "name": "contiamo-ui-components",
  "version": "0.0.1-47",
  "description": "A collection of primitive, composable UI components that make up Contiamo applications",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "git@github.com:Contiamo/ui-components.git",
  "author": "Tejas Kumar <tejas@contiamo.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.contiamo.com/"
  },
  "scripts": {
    "dev": "node scripts/dev-server/index.js",
    "package": "rm -rf lib && tsc -d --outDir './lib'",
    "package:watch": "tsc -d -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './index.ts' --fix && tslint './src/**/*.{ts,tsx}' --fix && tslint './utils/**/*.{ts,tsx}' --fix",
    "prettier": "prettier './index.ts' --write && prettier './src/**/*.{ts,tsx}' --write && prettier './utils/**/*.{ts,tsx}' --write",
    "precommit": "npm run test && npm run lint && npm run prettier && npm run package && git add .",
    "preversion": "npm run precommit",
    "version": "npm run package && git add lib"
  },
  "peerDependencies": {
    "glamor": "^2.20.40",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "attr-accept": "^1.1.0",
    "contiamo-ui-theme": "^0.0.1-45",
    "contiamo-ui-utils": "^0.0.1-45",
    "glamor": "^2.20.40",
    "glamorous": "^4.6.0",
    "moment": "^2.19.0",
    "react": "^16.0.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.0.0",
    "react-feather": "^1.0.7",
    "react-icons": "^2.2.5",
    "react-router-dom": "^4.2.2",
    "tinycolor2": "^1.4.1"
  }
}
