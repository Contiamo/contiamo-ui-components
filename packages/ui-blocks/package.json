{
  "name": "contiamo-ui-blocks",
  "version": "0.0.1-46",
  "description": "Opinionated composed ui components",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "git@github.com:Contiamo/ui-components.git",
  "author": "Peter Szerzo <peter@contiamo.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.contiamo.com/"
  },
  "scripts": {
    "dev": "node scripts/dev-server/index.js",
    "package": "rm -rf lib && tsc -d",
    "package:watch": "tsc -d -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './index.ts' --fix && tslint './src/**/*.{ts,tsx}' --fix && tslint './utils/**/*.{ts,tsx}' --fix",
    "prettier": "prettier './index.ts' --write && prettier './src/**/*.{ts,tsx}' --write",
    "precommit": "npm run test && npm run lint && npm run prettier && npm run package && git add .",
    "preversion": "npm run precommit",
    "version": "npm run package && git add lib"
  },
  "peerDependencies": {
    "glamor": "^2.20.40",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "contiamo-ui-components": "^0.0.1-46",
    "contiamo-ui-theme": "^0.0.1-45",
    "contiamo-ui-utils": "^0.0.1-45",
    "glamor": "^2.20.40",
    "glamorous": "^4.6.0",
    "moment": "^2.19.0",
    "react": "^16.0.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "setupFiles": [
      "./test-polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "../../node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.js"
    },
    "snapshotSerializers": [
      "../../node_modules/jest-serializer-enzyme"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "mapCoverage": true
  }
}
