{
  "name": "@operational/theme",
  "version": "0.1.0-16",
  "description": "Customization theme for the Operational UI packages.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "git@github.com:Contiamo/operational-ui.git",
  "author": "Peter Szerzo <peter@contiamo.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "package": "rm -rf lib && tsc -d",
    "package:watch": "tsc -d -w",
    "lint": "tslint './src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier './src/**/*.{ts,tsx}' --write",
    "test": "jest",
    "test:u": "jest -u"
  },
  "devDependencies": {
    "@types/core-js": "0.9.43",
    "@types/enzyme": "3.1.5",
    "@types/jest": "20.0.8",
    "@types/lodash": "4.14.74",
    "@types/marked": "0.3.0",
    "@types/node": "8.0.27",
    "@types/tinycolor2": "1.4.0",
    "awesome-typescript-loader": "3.4.1",
    "enzyme": "2.9.1",
    "enzyme-to-json": "1.5.1",
    "jest": "21.2.1",
    "jest-enzyme": "3.8.1",
    "jest-glamor-react": "3.1.0",
    "jest-serializer-enzyme": "1.0.0",
    "prettier": "1.8.2",
    "react-test-renderer": "16.0.0",
    "ts-jest": "21.0.0",
    "tslint": "5.7.0",
    "tslint-config-airbnb": "5.2.1",
    "tslint-config-prettier": "1.5.0",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-merge": "4.1.1"
  },
  "jest": {
    "setupFiles": [
      "./test-polyfills.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>/node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-enzyme"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme",
      "jest-enzyme"
    ],
    "mapCoverage": true
  }
}
