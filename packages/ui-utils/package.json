{
  "name": "contiamo-ui-utils",
  "version": "0.0.1-44",
  "description": "A collection of utilities used within the Contiamo UI suite",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": "git@github.com:Contiamo/ui-components.git",
  "author": "Tejas Kumar <tejas@contiamo.com>",
  "license": "MIT",
  "scripts": {
    "package": "rm -rf lib && tsc -d --outDir './lib'",
    "package:watch": "rm -rf lib && tsc -d -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier './src/**/*.{ts,tsx}' --write",
    "precommit": "npm run test && npm run lint && npm run prettier && npm run package && git add .",
    "preversion": "npm run precommit",
    "version": "npm run package && git add ."
  },
  "dependencies": {
    "contiamo-ui-theme": "^0.0.1-44",
    "glamor": "^2.20.40",
    "glamorous": "^4.9.7",
    "react": "^16.0.0",
    "tinycolor2": "^1.4.1"
  }
}
