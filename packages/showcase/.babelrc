{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ],
    "react",
    "flow",
    "stage-0"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "spec": true
          }
        ],
        "react",
        "stage-0"
      ]
    },
    "development": {
      "plugins": ["transform-runtime", "react-hot-loader/babel"]
    }
  }
}
