{
  "presets": [
    [
      "env",
      {
        "es2015": {
          "modules": false
        },
      },
    ],
    "react",
    "stage-0"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "development": {
      "plugins": ["transform-runtime","react-hot-loader/babel"],
    },
    "test": {
      "presets": [["env", { "spec": true }], "react", "stage-0"]
    }
  }
}
