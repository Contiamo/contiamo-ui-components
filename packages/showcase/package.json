{
  "name": "contiamo-ui-showcase",
  "version": "1.0.0",
  "description": "A showcase of the various components available in the Contiamo UI framework",
  "main": "index.js",
  "repository": "git@github.com:Contiamo/ui-components.git",
  "author": "Tejas Kumar <tejas@contiamo.com>",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --public --host 0.0.0.0",
    "build": "webpack -p",
    "flow": "flow",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ./src/**/* --ext .js,.jsx --fix",
    "precommit": "flow && npm run test && npm run lint"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-jest": "^20.0.3",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^4.5.0",
    "eslint-config-fbjs": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-relay": "^0.0.17",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "jest-enzyme": "^3.8.1",
    "jest-glamor-react": "^3.1.0",
    "jest-serializer-enzyme": "^1.0.0",
    "webpack-dashboard": "^1.0.0-5",
    "contiamo-ui-components": "^1.0.0",
    "contiamo-ui-utils": "^1.0.0",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "component-playground": "^2.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "glamor": "^2.20.40",
    "glamorous": "^4.6.0",
    "html-webpack-plugin": "^2.30.1",
    "jest-cli": "^20.0.4",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-feather": "^1.0.7",
    "react-hot-loader": "next",
    "react-icons": "^2.2.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "react-syntax-highlighter": "^5.7.0",
    "regenerator-runtime": "^0.11.0",
    "webpack": "^3.5.5",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./node_modules/jest-enzyme/lib/index.js",
    "moduleNameMapper": {
      "\\.(css|jpg|png)$": "<rootDir>/empty-module.js",
      "\\.snippet.(js|jsx)$": "<rootDir>/flow/raw-import.js"
    },
    "snapshotSerializers": ["<rootDir>/node_modules/jest-serializer-enzyme"],
    "unmockedModulePathPatterns": ["react", "enzyme", "jest-enzyme"]
  }
}
